{
  "env": {
    "browser" : true,
    "node"    : true,
    "es6"     : true,
    "mocha"   : true,
    "jasmine" : true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "strict"                     : [2, "global"],
    "no-catch-shadow"            : 2,
    "no-delete-var"              : 2,
    "no-label-var"               : 2,
    "no-shadow"                  : 2,
    "no-shadow-restricted-names" : 2,
    "no-undef"                   : 2,
    "no-undef-init"              : 2,
    "no-undefined"               : 2,
    "no-unused-vars"             : 2,
    "no-use-before-define"       : 2,
    "no-unused-vars"             : [2, {"vars" : "all", "args" : "after-used"}]
  }
}
